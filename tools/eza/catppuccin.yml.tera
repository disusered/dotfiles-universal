  compressed: { foreground: "{{ pink_hex_upper }}" }
  temp: { foreground: "{{ maroon_hex_upper }}" }
  compiled: { foreground: "{{ sapphire_hex_upper }}" }
  build: { foreground: "{{ surface2_hex_upper }}" }
  source: { foreground: "{{ blue_hex_upper }}" }

