# vi: ft=yaml
colourful: true

filekinds:
  normal: { foreground: "{{ subtext1_hex_upper }}" }
  directory: { foreground: "{{ accent_color_hex_upper }}" }
  symlink: { foreground: "{{ sky_hex_upper }}" }
  pipe: { foreground: "{{ overlay1_hex_upper }}" }
  block_device: { foreground: "{{ maroon_hex_upper }}" }
  char_device: { foreground: "{{ maroon_hex_upper }}" }
  socket: { foreground: "{{ surface2_hex_upper }}" }
  special: { foreground: "{{ mauve_hex_upper }}" }
  executable: { foreground: "{{ green_hex_upper }}" }
  mount_point: { foreground: "{{ sapphire_hex_upper }}" }

perms:
  user_read: { foreground: "{{ text_hex_upper }}" }
  user_write: { foreground: "{{ yellow_hex_upper }}" }
  user_execute_file: { foreground: "{{ green_hex_upper }}" }
