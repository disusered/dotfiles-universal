  number_mega: { foreground: "{{ blue_hex_upper }}" }
  number_giga: { foreground: "{{ mauve_hex_upper }}" }
  number_huge: { foreground: "{{ mauve_hex_upper }}" }
  unit_byte: { foreground: "{{ subtext0_hex_upper }}" }
  unit_kilo: { foreground: "{{ blue_hex_upper }}" }
  unit_mega: { foreground: "{{ mauve_hex_upper }}" }
  unit_giga: { foreground: "{{ mauve_hex_upper }}" }
  unit_huge: { foreground: "{{ sapphire_hex_upper }}" }

